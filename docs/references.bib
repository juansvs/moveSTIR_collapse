editor = {Freckleton, Robert},
file = {:C$\backslash$:/Users/juans/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Calabrese, Fleming, Gurarie - 2016 - ctmm an R package for analyzing animal relocation data as a continuous-time stochastic process.pdf:pdf},
issn = {2041-210X},
journal = {Methods in Ecology and Evolution},
keywords = {Key-words: autocorrelated kernel density estimatio,home range estimation,non-Markovian maximum likelihood,periodogram analysis,tracking data,variogram analysis},
month = {sep},
number = {9},
pages = {1124--1132},
title = {ctmm: an {\textless}scp{\textgreater}r{\textless}/scp{\textgreater} package for analyzing animal relocation data as a continuous‐time stochastic process},
url = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/2041-210X.12559 https://onlinelibrary.wiley.com/doi/10.1111/2041-210X.12559},
volume = {7},
