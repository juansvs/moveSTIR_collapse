issn = {20513933},
journal = {Movement Ecology},
keywords = {Central place foraging,Classification scheme,Cluster analysis,GPS data,Migration,Movement ecology,Nomadism,Territoriality},
month = {jun},
number = {1},
publisher = {BioMed Central Ltd.},
title = {{Suite of simple metrics reveals common movement syndromes across vertebrate taxa}},
url = {https://click.endnote.com/viewer?doi=10.1186{\%}2Fs40462-017-0104-2{\&}token=WzI0NjA5NTUsIjEwLjExODYvczQwNDYyLTAxNy0wMTA0LTIiXQ.XbFlgdRwudo9gsGe0DVY{\_}q8xcT4},
volume = {5},
