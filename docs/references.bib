journal = {Biological Reviews},
keywords = {behavioural ecology,collective movement,ecological niche,social environment,social network,social niche,social phenotype,spatial environment,spatial phenotype},
month = {jun},
number = {3},
pages = {868--886},
publisher = {John Wiley {\&} Sons, Ltd},
title = {{Behavioural ecology at the spatial–social interface}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1111/brv.12934 https://onlinelibrary.wiley.com/doi/abs/10.1111/brv.12934 https://onlinelibrary.wiley.com/doi/10.1111/brv.12934},
