author = {Manlove, Kezia and Aiello, Christina and Sah, Pratha and Cummins, Bree and Hudson, Peter J. and Cross, Paul C.},
doi = {10.1098/rspb.2018.0670},
file = {:Users/mqwilber/Dropbox/Documents/Papers_and_Books/Manloveetal2018_ProcB.pdf:pdf},
isbn = {0000000272},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Lagrangian movement,contact network,network projection,pathogen transmission,tripartite tagging network},
mendeley-groups = {Disease_Ecology,Projects_and_Permits/direct_indirect},
