issn = {2041210X},
journal = {Methods in Ecology and Evolution},
keywords = {Key-words: autocorrelation function,Mongo-lian gazelle,Procapra gutturosa,characteristic scale,home range,irregular sampling,movement ecology,satellite collar movement data,time series,tracking data gaps},
month = {may},
