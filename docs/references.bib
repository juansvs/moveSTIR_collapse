pages = {16--29},
pmid = {36321473},
publisher = {John Wiley {\&} Sons, Ltd},
title = {{How to scale up from animal movement decisions to spatiotemporal patterns: An approach via step selection}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1111/1365-2656.13832 https://onlinelibrary.wiley.com/doi/abs/10.1111/1365-2656.13832 https://besjournals.onlinelibrary.wiley.com/doi/10.1111/1365-2656.13832},
