issn = {13652435},
journal = {Functional Ecology},
keywords = {Batrachochytrium dendrobatidis,behaviour,disease risk,environmental heterogeneity,habitat use,host behaviour,host–parasite interactions},
month = {may},
number = {5},
pages = {1262--1270},
publisher = {Blackwell Publishing Ltd},
title = {{Routine habitat switching alters the likelihood and persistence of infection with a pathogenic parasite}},
